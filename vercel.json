{
  "regions": ["bom1"],
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  },
  "build": {
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ],
  "public": true,
  "bypassToken": {
    "exempt": [
      "/api/v1/ingest",
      "/api/rrweb-events",
      "/tracker.js"
    ]
  }
}